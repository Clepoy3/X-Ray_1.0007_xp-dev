function gen_custom_data(tbl) counterAdd("amk.gen_custom_data")
	local str = ""
	for key, value in pairs(tbl) do
		str = str.."\n["..key.."]\n"
		for k, v in pairs(value) do
			if v~="<<no_value>>" then str=str..k.." = "..v.."\n"
			else str=str..k.."\n"
			end
		end
	end
	return str
end 

