-----------------------------------------------------------
function del_obj_by_name( obj_name )
	return amk.release(obj_name)
end
-----------------------------------------------------------
-- Удаление объекта по иду
-----------------------------------------------------------
function del_obj_by_sid( story_id, news_str )
	local se_obj = alife():story_object(story_id)
	if se_obj then
		if news_str then
		sol_utils.send_tip( news_str, nil, nil, 25000)
	end
	end
	return amk.release(story_id)
end

-----------------------------------------------------------
-- Действия по взятию предмета
-----------------------------------------------------------
function checking_taken_item( obj )	
	local str_del_zone = "Аномальная зона деактивирована"
	if obj:section()=="video_kasseta"		then del_obj_by_sid(17019,"Найдена видеокассета")
	elseif obj:section()=="kluch_dell_teleport"	then del_obj_by_sid(17000,"Телепорт на входе в базу отключен")
	elseif obj:section()=="kluch_dell_teleport1"	then del_obj_by_sid(17001,"Телепорт на выходе из базы отключен")
	elseif obj:section()=="mini_antenna1"		then del_obj_by_sid(18104,str_del_zone) 
	elseif obj:section()=="mini_antenna2"		then del_obj_by_sid(18105,str_del_zone)
	elseif obj:section()=="mini_antenna3"		then del_obj_by_sid(18106,str_del_zone)
	elseif obj:section()=="mini_antenna4"		then del_obj_by_sid(18107,str_del_zone)
	elseif obj:section()=="mini_antenna5"		then del_obj_by_sid(18108,str_del_zone)
	elseif obj:section()=="af_9x39_izomorf"		then del_obj_by_sid(17023,"Телепорт, блокирующий рычаг, отключен")
	elseif obj:section() == "mutant_flesh_wpn" then
		amk.drop_item(db.actor,obj)
		amk.remove_item(obj)
		amk.spawn_item_in_inv("wpn_desert_eagle") 
	end
end

