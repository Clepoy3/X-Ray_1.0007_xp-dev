 local obj, item_clsid, item_sect
 local se_box = sim:object("lima_podval_seif")
 local box = nil
 if se_box then box = level.object_by_id(se_box.id) end

 for i=0, actor:object_count()-1 do
  obj = actor:object(i)
  if obj then
   item_clsid = obj:clsid()
   item_sect  = obj:section()
   if item_sect == "arhara_obman" or item_sect == "shaxter_tele" or 
      item_sect == "hand_teleporter" or 
      item_sect == "hand_teleporter_arhara" or item_sect == "hand_teleporter_fake" or item_sect == "ex_hoard_tip12" then
	if box then
		actor:transfer_item(obj, box)
	end	
   elseif ( IAmAWeaponFireOnly[item_clsid] and 
	    (item_clsid ~= clsid.wpn_pm_s or item_sect == "wpn_colt1911" or item_sect == "wpn_colt_m1" or item_sect == "wpn_desert_eagle" or item_sect == "wpn_eagle_m1" or item_sect == "wpn_raging_bull") and 
	    item_clsid ~= clsid.wpn_fort and item_clsid ~= clsid.wpn_walther_s and item_clsid ~= clsid.wpn_hpsa_s and
	    (item_clsid ~= clsid.wpn_bm16_s or item_sect  ~= "wpn_bm16") ) or 
 	  ( (not outfit or obj:id() ~= outfit:id()) and 
	    (item_clsid == clsid.equ_exo or item_clsid == clsid.equ_military or item_clsid == clsid.equ_scientific or item_clsid == clsid.equ_stalker or item_clsid == clsid.equ_stalker_s) ) then
	sim:release(sim:object(obj:id()), true)
   end 
  end
 end
